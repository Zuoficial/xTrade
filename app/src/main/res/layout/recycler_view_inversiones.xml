<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="250dp"
    android:layout_height="173dp"
    android:layout_marginBottom="20dp"
    android:layout_marginEnd="10dp"
    android:layout_marginStart="50dp"
    android:background="@drawable/fondo_marcador_neutral">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inversion origen"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        android:gravity="center_horizontal"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp" />

    <TextView
        android:id="@+id/inversionRV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
      android:gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:text="999.00009999"
        android:textColor="@android:color/white"
        android:textSize="24dp"
        android:maxLines="1"/>


    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cantidad destino"
        android:gravity="center_horizontal"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="60dp"/>

    <TextView
        android:id="@+id/cantidadRV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginTop="80dp"
        android:text="999.00009999"
        android:textColor="@android:color/white"
        android:textSize="24dp"
        android:maxLines="1"/>


    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Precio"
        android:gravity="center_horizontal"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="112dp"/>

    <TextView
        android:id="@+id/precioRV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginTop="130dp"
        android:text="999.00009999"
        android:textColor="@android:color/white"
        android:textSize="24dp"
        android:maxLines="1"/>




</RelativeLayout>